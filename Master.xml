  <listrepeat><![CDATA[
        <title> [makelist.param3]</title>
        <link>$doregex[getUrl]</link>
		<thumbnail>$doregex[poster]</thumbnail>
		<info>$doregex[plot]</info>
        ]]></listrepeat>
<expres><![CDATA[href="\/.*,(.*?),list.*\n.*\n.*\n.*href="\/([^"]*)".*?>(.*?)<]]></expres>
<page>[makelist3.param1]/list,1,15</page>
<cookieJar></cookieJar>
